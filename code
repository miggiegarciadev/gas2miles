const zeroFuel = (distanceToPump, mpg, fuelLeft) => {
  console.log(distanceToPump, mpg, fuelLeft)

  if (distanceToPump / mpg == fuelLeft || distanceToPump / mpg <= fuelLeft){
    return true
  } else {
    return false
  }
};
